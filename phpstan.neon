    - '#Cannot access property \$[a-zA-Z_]+ on App\\[a-zA-Z]+\|Illuminate\\Database\\Eloquent\\Builder\|null\.#'
    - '#Cannot access property \$[a-zA-Z_]+ on App\\[a-zA-Z]+\|null\.#'
