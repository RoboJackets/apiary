includes:
  - %rootDir%/../phpstan-strict-rules/rules.neon
  - %rootDir%/../phpstan-deprecation-rules/rules.neon
  - %rootDir%/../../nunomaduro/larastan/extension.neon
parameters:
  autoload_directories:
    - database/seeds/
  excludes_analyse:
    - *.blade.php
    - .phpstorm.meta.php
    - _ide_helper.php
    - bootstrap/*
    - storage/*
    - vendor/*
