{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "apereo/phpcas": "1.3.8",
        "chelout/laravel-relationship-events": "1.1.1",
        "doctrine/dbal": "2.10.1",
        "laravel/framework": "6.12.0",
        "adldap2/adldap2-laravel": "6.0.8",
        "barryvdh/laravel-ide-helper": "2.6.6",
        "bugsnag/bugsnag-laravel": "2.17.1",
        "guzzlehttp/guzzle": "6.5.2",
        "laravel/horizon": "3.4.7",
        "laravel/nova": "2.9.4",
        "laravel/slack-notification-channel": "2.0.2",
        "laravel/socialite": "4.3.1",
        "laravel/tinker": "1.0.10",
        "maatwebsite/laravel-nova-excel": "1.1.9",
        "predis/predis": "1.1.1",
        "spatie/laravel-permission": "3.6.0",
        "vyuldashev/nova-permission": "2.5.0",
        "robojackets/error-pages": "2.0.0",
        "robojackets/network-check": "1.0.1",
        "spatie/laravel-failed-job-monitor": "3.2.0",
        "spatie/laravel-sluggable": "2.2.1",
        "square/connect": "2.20190710.0",
        "subfission/cas": "3.0",
        "uxweb/sweet-alert": "dev-master"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "1.3.0",
        "filp/whoops": "2.7.1",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "3.0.1",
        "facade/ignition": "1.16.0",
        "phpunit/phpunit": "8.5.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-package-update": [
            "rm -rf error-pages-cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://packages.robojackets.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/regnilk/cas"
        }
    ]
}
