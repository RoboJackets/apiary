{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "apereo/phpcas": "1.3.8",
        "barryvdh/laravel-ide-helper": "2.8.0",
        "bugsnag/bugsnag-laravel": "2.19.0",
        "chelout/laravel-relationship-events": "1.2.1",
        "doctrine/dbal": "2.10.2",
        "guzzlehttp/guzzle": "6.5.5",
        "laravel/framework": "^7.25",
        "laravel/horizon": "^4.0",
        "laravel/nova": "^3.0",
        "laravel/slack-notification-channel": "2.1.0",
        "laravel/socialite": "4.4.1",
        "laravel/tinker": "2.4.2",
        "laravel/vapor-cli": "1.8.0",
        "laravel/vapor-core": "2.8.2",
        "oitnetworkservices/buzzapiclient": "0.1.1",
        "predis/predis": "1.1.2",
        "robojackets/error-pages": "2.1.1",
        "robojackets/network-check": "1.0.1",
        "spatie/laravel-failed-job-monitor": "3.3.1",
        "spatie/laravel-permission": "3.15.0",
        "spatie/laravel-sluggable": "^2.4",
        "square/connect": "2.20190710.0",
        "subfission/cas": "3.1.0",
        "uxweb/sweet-alert": "dev-master",
        "vyuldashev/nova-permission": "2.9.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "1.3.0",
        "facade/ignition": "^2.0",
        "filp/whoops": "2.7.3",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "0.6.2",
        "ocramius/package-versions": "1.9.0",
        "phpstan/extension-installer": "1.0.4",
        "psalm/plugin-laravel": "1.3.1",
        "robojackets/static-analysis": "1.0.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "process-timeout": 10,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/regnilk/cas"
        }
    ]
}
