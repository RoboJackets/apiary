{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "apereo/phpcas": "1.3.8",
        "barryvdh/laravel-ide-helper": "2.8.2",
        "bugsnag/bugsnag-laravel": "2.21.0",
        "chelout/laravel-relationship-events": "1.4.0",
        "doctrine/dbal": "2.12.1",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "2.0.3",
        "guzzlehttp/guzzle": "7.2.0",
        "laravel/framework": "8.18.1",
        "laravel/horizon": "5.6.1",
        "laravel/nova": "3.16.3",
        "laravel/slack-notification-channel": "2.3.0",
        "laravel/socialite": "5.1.2",
        "laravel/tinker": "2.5.0",
        "oitnetworkservices/buzzapiclient": "0.1.3",
        "predis/predis": "1.1.6",
        "robojackets/error-pages": "2.1.1",
        "robojackets/network-check": "1.0.1",
        "sensiolabs/security-checker": "6.0.3",
        "spatie/laravel-failed-job-monitor": "3.4.0",
        "spatie/laravel-permission": "3.18.0",
        "square/connect": "2.20190710.0",
        "subfission/cas": "4.0.0",
        "ukfast/laravel-health-check": "1.8.0",
        "uxweb/sweet-alert": "dev-master",
        "vyuldashev/nova-permission": "2.10.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "1.6.0",
        "facade/ignition": "2.5.3",
        "filp/whoops": "2.9.1",
        "mockery/mockery": "1.4.2",
        "nunomaduro/collision": "5.1.0",
        "nunomaduro/larastan": "0.6.11",
        "phpstan/extension-installer": "1.0.6",
        "psalm/plugin-laravel": "1.4.1",
        "robojackets/static-analysis": "1.1",
        "fakerphp/faker": "1.12.1",
        "phpunit/phpunit": "9.5.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "process-timeout": 10,
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org",
            "only": [
                "robojackets/*",
                "oitnetworkservices/*"
            ]
        }
    ]
}
