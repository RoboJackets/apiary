{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-ldap": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-redis": "*",
        "ext-sqlite3": "*",
        "ext-uuid": "*",
        "ext-xml": "*",
        "adldap2/adldap2-laravel": "6.1.6",
        "apereo/phpcas": "1.5.0",
        "arcanedev/laravel-markdown": "6.0.0",
        "barryvdh/laravel-dompdf": "2.0.0",
        "chelout/laravel-relationship-events": "1.5.0",
        "doctrine/dbal": "3.3.7",
        "enlightn/enlightn": "2.1.0",
        "guzzlehttp/guzzle": "7.4.5",
        "htmlmin/htmlmin": "9.0.1",
        "http-interop/http-factory-guzzle": "1.2.0",
        "kiritokatklian/nova-permission": "4.0.3",
        "laravel/framework": "9.22.1",
        "laravel/horizon": "5.9.10",
        "laravel/nova": "4.12.13",
        "laravel/passport": "10.4.1",
        "laravel/scout": "9.4.10",
        "laravel/slack-notification-channel": "2.4.0",
        "laravel/socialite": "5.5.3",
        "laravel/tinker": "2.7.2",
        "maatwebsite/excel": "3.1.40",
        "madewithlove/laravel-nova-uuid-support": "1.4",
        "meilisearch/meilisearch-php": "0.24.0",
        "oitnetworkservices/buzzapiclient": "0.1.4",
        "predis/predis": "1.1.10",
        "realrashid/sweet-alert": "5.1.0",
        "robojackets/auth-stickler": "0.5",
        "robojackets/meilisearch-index-settings-helper": "0.7.0",
        "sentry/sentry-laravel": "2.13.0",
        "smalot/pdfparser": "2.2.1",
        "spatie/laravel-csp": "2.7.0",
        "spatie/laravel-failed-job-monitor": "4.1.1",
        "spatie/laravel-permission": "5.5.5",
        "spatie/laravel-webhook-client": "3.1.2",
        "spatie/once": "3.1.0",
        "square/square": "20.1.0.20220720",
        "subfission/cas": "4.2.0",
        "symfony/http-client": "6.1.3",
        "symfony/postmark-mailer": "6.1.0",
        "ukfast/laravel-health-check": "1.13.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.12.3",
        "beyondcode/laravel-dump-server": "1.8.0",
        "fakerphp/faker": "1.20.0",
        "filp/whoops": "2.14.5",
        "mockery/mockery": "1.5.0",
        "mxl/laravel-job": "1.3.0",
        "nunomaduro/collision": "6.2.1",
        "nunomaduro/larastan": "2.1.12",
        "phan/phan": "5.3.2",
        "phpstan/extension-installer": "1.1.0",
        "psalm/plugin-laravel": "2.0.1",
        "robojackets/static-analysis": "1.4",
        "slevomat/coding-standard": "7.2.1",
        "spatie/ray": "1.34.5",
        "spatie/laravel-ignition": "1.3.1",
        "phpunit/phpunit": "9.5.21"
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-messageformatter": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-util": "*",
        "symfony/polyfill-uuid": "*",
        "paragonie/random_compat": "*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "ci": [
            "vendor/bin/phpcs",
            "vendor/bin/phpstan analyse --level=max .",
            "vendor/bin/phan",
            "vendor/bin/psalm"
        ],
        "test": [
            "rm -f storage/*.key",
            "php artisan passport:keys --no-interaction",
            "cp storage/db-clean.sqlite storage/db-testing.sqlite",
            "php artisan test"
        ]
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "process-timeout": 90,
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org",
            "only": [
                "robojackets/*",
                "oitnetworkservices/*"
            ]
        }
    ]
}
