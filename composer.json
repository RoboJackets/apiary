{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "64robots/nova-date-filter": "0.3.2",
        "apereo/phpcas": "1.3.7",
        "barryvdh/laravel-ide-helper": "2.6.2",
        "bugsnag/bugsnag-laravel": "2.15.2",
        "chelout/laravel-relationship-events": "v0.6.3",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "4.1.0",
        "guzzlehttp/guzzle": "6.3.3",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "2.2.2",
        "laravel/nova": "1.3.2",
        "laravel/tinker": "1.0.8",
        "maatwebsite/laravel-nova-excel": "1.1.4",
        "predis/predis": "1.1.1",
        "spatie/laravel-failed-job-monitor": "3.0.3",
        "spatie/laravel-permission": "2.37.0",
        "spatie/laravel-sluggable": "2.1.7",
        "square/connect": "2.20190508.0",
        "subfission/cas": "dev-master",
        "uxweb/sweet-alert": "1.4.3",
        "vyuldashev/nova-permission": "1.7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "0.9.11",
        "phpunit/phpunit": "6.5.14",
        "filp/whoops": "2.3.1",
        "nunomaduro/collision": "1.1.22"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
