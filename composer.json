{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-curl": "^7.4",
        "ext-gd": "^7.4",
        "ext-intl": "^7.4",
        "ext-mbstring": "^7.4",
        "apereo/phpcas": "1.4.0",
        "arcanedev/laravel-markdown": "5.1.0",
        "barryvdh/laravel-dompdf": "0.9.0",
        "chelout/laravel-relationship-events": "1.4.0",
        "doctrine/dbal": "2.13.7",
        "guzzlehttp/guzzle": "7.4.1",
        "http-interop/http-factory-guzzle": "1.2.0",
        "laravel/framework": "8.79.0",
        "laravel/horizon": "5.8.0",
        "laravel/nova": "3.30.0",
        "laravel/passport": "10.3.0",
        "laravel/scout": "9.3.4",
        "laravel/slack-notification-channel": "2.3.1",
        "laravel/socialite": "5.2.6",
        "laravel/tinker": "2.7.0",
        "lynndigital/select-or-custom": "dev-master",
        "maatwebsite/excel": "3.1.35",
        "madewithlove/laravel-nova-uuid-support": "1.3",
        "meilisearch/meilisearch-php": "0.20.0",
        "oitnetworkservices/buzzapiclient": "0.1.3",
        "outhebox/nova-hidden-field": "1.0.3",
        "predis/predis": "1.1.10",
        "robojackets/auth-stickler": "0.5",
        "robojackets/meilisearch-index-settings-helper": "0.5.0",
        "sentry/sentry-laravel": "2.10.2",
        "spatie/laravel-failed-job-monitor": "3.4.0",
        "spatie/laravel-permission": "3.18.0",
        "spatie/laravel-webhook-client": "2.7.5",
        "spatie/once": "2.2.1",
        "square/square": "17.0.0.20211215",
        "subfission/cas": "4.1.0",
        "ukfast/laravel-health-check": "1.13.2",
        "uxweb/sweet-alert": "2.0.5",
        "vyuldashev/nova-permission": "2.11.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.11.0",
        "beyondcode/laravel-dump-server": "1.7.0",
        "facade/ignition": "2.17.4",
        "fakerphp/faker": "1.17.0",
        "filp/whoops": "2.14.5",
        "mockery/mockery": "1.4.4",
        "mxl/laravel-job": "1.2.0",
        "nunomaduro/collision": "5.11.0",
        "nunomaduro/larastan": "0.7.15",
        "phan/phan": "4.1.0",
        "phpstan/extension-installer": "1.1.0",
        "psalm/plugin-laravel": "1.4.6",
        "robojackets/static-analysis": "1.4",
        "slevomat/coding-standard": "6.4.1",
        "spatie/ray": "1.32.3"
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-messageformatter": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-util": "*",
        "symfony/polyfill-uuid": "*",
        "paragonie/random_compat": "*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "ci": [
            "vendor/bin/phpcs",
            "vendor/bin/phpstan analyse --level=max .",
            "vendor/bin/phan",
            "vendor/bin/psalm"
        ],
        "test": [
            "php artisan passport:keys --no-interaction",
            "cp storage/db-clean.sqlite storage/db-testing.sqlite",
            "php artisan test"
        ]
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "process-timeout": 20,
        "platform": {
            "php": "7.4.13"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org",
            "only": [
                "robojackets/*",
                "oitnetworkservices/*"
            ]
        }
    ]
}
