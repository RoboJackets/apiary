{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-ldap": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-redis": "*",
        "ext-sqlite3": "*",
        "ext-uuid": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "adldap2/adldap2-laravel": "6.1.7",
        "ans-group/laravel-health-check": "1.14.0",
        "apereo/phpcas": "1.6.1",
        "ashallendesign/laravel-config-validator": "2.4.0",
        "barryvdh/laravel-dompdf": "2.0.1",
        "chelout/laravel-relationship-events": "2.0.0",
        "doctrine/dbal": "3.6.2",
        "enlightn/enlightn": "2.3.0",
        "fakerphp/faker": "1.21.0",
        "guzzlehttp/guzzle": "7.5.1",
        "htmlmin/htmlmin": "dev-master",
        "http-interop/http-factory-guzzle": "1.2.0",
        "kiritokatklian/nova-permission": "4.0.5",
        "laravel/framework": "10.8.0",
        "laravel/horizon": "5.15.0",
        "laravel/nova": "4.23.0",
        "laravel/passport": "11.8.5",
        "laravel/scout": "10.1.1",
        "laravel/slack-notification-channel": "2.5.0",
        "laravel/socialite": "5.6.1",
        "laravel/tinker": "2.8.1",
        "maatwebsite/excel": "3.1.48",
        "madewithlove/laravel-nova-uuid-support": "dev-l10-compatibility",
        "meilisearch/meilisearch-php": "1.1.1",
        "mrclay/jsmin-php": "2.4.3",
        "oitnetworkservices/buzzapiclient": "0.1.4",
        "realrashid/sweet-alert": "6.0.0",
        "sentry/sentry-laravel": "3.3.3",
        "smalot/pdfparser": "2.4.0",
        "spatie/laravel-csp": "2.8.3",
        "spatie/laravel-failed-job-monitor": "4.2.1",
        "spatie/laravel-permission": "5.10.1",
        "spatie/laravel-webhook-client": "3.1.7",
        "spatie/once": "3.1.0",
        "square/square": "26.0.0.20230419",
        "subfission/cas": "4.4.0",
        "symfony/http-client": "6.2.9",
        "symfony/postmark-mailer": "6.2.7",
        "xammie/mailbook": "1.6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "dev-master as 2.12.4",
        "beyondcode/laravel-dump-server": "1.9.0",
        "filp/whoops": "2.15.2",
        "laravel/pint": "1.9.0",
        "mockery/mockery": "1.5.1",
        "mxl/laravel-job": "1.4.0",
        "nunomaduro/collision": "7.5.0",
        "nunomaduro/larastan": "2.6.0",
        "phan/phan": "5.4.2",
        "phpstan/extension-installer": "1.3.0",
        "phpstan/phpstan": "1.10.14",
        "phpstan/phpstan-deprecation-rules": "1.1.3",
        "phpstan/phpstan-strict-rules": "1.5.1",
        "phpunit/phpunit": "10.1.1",
        "psalm/plugin-laravel": "2.8.0",
        "slevomat/coding-standard": "8.11.0",
        "spatie/laravel-ignition": "2.1.0",
        "spatie/ray": "1.37.1",
        "squizlabs/php_codesniffer": "3.7.2",
        "vimeo/psalm": "5.9.0"
    },
    "replace": {
        "symfony/polyfill-apcu": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-messageformatter": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php54": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-util": "*",
        "symfony/polyfill-uuid": "*",
        "paragonie/random_compat": "*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "sed -i 's/getHtmlBody/getTextBody/' vendor/xammie/mailbook/src/ResolvedMail.php",
            "sed -i \"s/return \\$html;/return \\'<pre>\\'.\\$html.\\'<\\/pre>\\';/\" vendor/xammie/mailbook/src/ResolvedMail.php",
            "sed -i 's/@template/@nothing/' vendor/laravel/nova/src/PerformsValidation.php"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "ci": [
            "vendor/bin/pint --test",
            "vendor/bin/phpcs",
            "vendor/bin/phpstan analyse --level=max .",
            "vendor/bin/phan",
            "vendor/bin/psalm"
        ],
        "test": [
            "rm -f storage/*.key",
            "php artisan passport:keys --no-interaction",
            "cp storage/db-clean.sqlite storage/db-testing.sqlite",
            "vendor/phpunit/phpunit/phpunit"
        ]
    },
    "config": {
        "classmap-authoritative": true,
        "preferred-install": "dist",
        "process-timeout": 90,
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.1"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com",
            "only": [
                "laravel/nova"
            ]
        },
        {
            "type": "composer",
            "url": "https://composer.robojackets.org",
            "only": [
                "oitnetworkservices/buzzapiclient"
            ]
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-nova-uuid-support.git",
            "only": [
                "madewithlove/laravel-nova-uuid-support"
            ]
        }
    ]
}
